
@{
    ViewBag.Title = "index";
}
<div class="container">
    <h2>JAVASCRIPT</h2>
    <div class="row">
        @foreach (var item in ViewBag.Url)
        {
            <div class="col-md-4">
                <ul class="list-group">
                    <li class="list-group-item">@item.Name</li>
                    <li class="list-group-item" style="overflow-x:hidden;"><a href="@item.Link">@item.Link</a></li>
                </ul>
            </div>
        }
    </div>
</div>
<div class="container">
    <h2>NODE.JS</h2>
    <div class="row">
        @foreach (var item in ViewBag.node)
        {
            <div class="col-md-4">
                <ul class="list-group">
                    <li class="list-group-item">@item.Name</li>
                    <li class="list-group-item" style="overflow-x:auto;"><a href="@item.Link">@item.Link</a></li>
                </ul>
            </div>
        }
    </div>
</div>
<div class="container">
    <h2>WEBPACK</h2>
    <div class="row">
        @foreach (var item in ViewBag.webpack)
        {
            <div class="col-md-4">
                <ul class="list-group">
                    <li class="list-group-item">@item.Name</li>
                    <li class="list-group-item" style="overflow-x:auto;"><a href="@item.Link">@item.Link</a></li>
                </ul>
            </div>
        }
    </div>
</div>
